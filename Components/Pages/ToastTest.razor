@page "/toast"
@inject IToastService service
@rendermode InteractiveServer

<FluentButton OnClick=test>Test</FluentButton>

@code {
    private void test() {
        Console.WriteLine("hi");
        service.ShowToast(ToastIntent.Success, "test", 10_000_000, "Do thing", EventCallback.Factory.Create<ToastResult>(this, callback));
    }

    private void callback(ToastResult result) {
        Console.WriteLine("callback");
    }
}
